<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" minHeight="880.0" minWidth="800.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.tichawa.cis.config.DataSheetController">
  <children>
    <GridPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="400.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="400.0" />
      </columnConstraints>
      <rowConstraints>
        <RowConstraints vgrow="NEVER" />
        <RowConstraints vgrow="SOMETIMES" />
        <RowConstraints vgrow="ALWAYS" />
        <RowConstraints vgrow="ALWAYS" />
      </rowConstraints>
      <children>
        <MenuBar id="Menu" prefHeight="20.0" GridPane.columnSpan="2">
          <menus>
            <Menu fx:id="File">
              <items>
                <MenuItem fx:id="Print" onAction="#print" />
              </items>
            </Menu>
            <Menu fx:id="Lang">
              <items>
                <MenuItem fx:id="SwitchLang" onAction="#switchLang" />
              </items>
            </Menu>
          </menus>
        </MenuBar>
        <TextArea id="Header" fx:id="Header" GridPane.columnSpan="2" GridPane.rowIndex="1" maxWidth="Infinity" maxHeight="50.0" text="Header" />
        <TextArea id="Specs" fx:id="Specs" maxHeight="Infinity" text="Specs" GridPane.rowIndex="2" />
        <ImageView id="ProductPic" fx:id="ProductPic" GridPane.columnIndex="1" GridPane.rowIndex="2" GridPane.valignment="TOP" GridPane.halignment="RIGHT" />
        <ImageView id="ProfilePic" fx:id="ProfilePic" GridPane.rowIndex="3" GridPane.valignment="BOTTOM" GridPane.halignment="LEFT" />
        <TextArea id="CLConfig" fx:id="CLConfig" maxHeight="Infinity" text="CLConfig" GridPane.columnIndex="1" GridPane.rowIndex="3" />
      </children>
    </GridPane>
  </children>
</AnchorPane>
